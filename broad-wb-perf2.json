{
  "name": "terra",
  "owner": "broad-wb-perf2",
  "intent": "terra infrastructure",
  "production": false,
  "stability": "volatile",
  "global_vars": {
    "vault": {
      "policy": "dsde-write"
    },
    "env": {
      "VAULT_PATH_PREFIX": "secret/dsde/firecloud/ephemeral/broad-wb-perf2"
    },
    "terraform": {
      "docker_image": "us.gcr.io/broad-dsp-gcr-public/config-render:rl_7",
      "providers": {
        "google": {
          "default": {
            "project": "broad-wb-perf2",
            "vault_sa_path": "secret/dsde/terra/envs/common/terra-env-owner-service-account.json",
            "region": "us-central1"
          }
        }
      },
      "backend": {
        "vault_sa_path": "secret/dsp/service-accts/dsp-techops/terraform-state-service-account.json",
        "state_path_prefix":"workbench",
        "state_bucket": "broad-dsp-terraform-state"
      }
    }
  },
  "profile_vars": {
    "gsuite-intermediate-domain": {
      "vault": {
        "policy": "vault-admins"
      }
    },
    "gsuite": {
      "terraform": {
        "providers": {
          "google": {
            "intermediate-domain-delegator": {
              "project": "broad-dsp-techops-dev",
              "vault_sa_path": "secret/devops/terraform/dev/dsp-techops/credentials",
              "region": "us-central1"
            }
          }
        }
      }
    },
    "thurloe-configs": {
      "env": {
        "APP_NAME": "thurloe",
        "FIRECLOUD_DEVELOP_GIT_BRANCH": "dev",
        "SERVICE_GIT_SHA_12_CHAR": "4c11a7177674"
      }
    },
    "opendj-configs": {
      "env": {
        "APP_NAME": "opendj",
        "FIRECLOUD_DEVELOP_GIT_BRANCH": "perf-146-sam-opendj"
      }
    },
    "sam":{
      "env": {
        "SAM_INSTANCE_NUM_HOSTS": "1"
      }
    },
    "sam-configs": {
      "env": {
        "APP_NAME": "sam",
        "FIRECLOUD_DEVELOP_GIT_BRANCH": "perf-146-sam-opendj",
        "SERVICE_GIT_SHA_12_CHAR": "cc43b109de12"
      }
    }
  },
  "environments": {
    "default": {
      "name": "broad-wb-perf2"
    }
  },
  "projects": {
    "default": {
      "name": "broad-wb-perf2",
      "provider_sa_path": "secret/dsde/terra/envs/common/terra-env-owner-service-account.json"
    }
  },
  "regions": {
    "default": {
      "name": "us-central1",
      "zone_name": "us-central1-a"
    }
  },
  "dns": {
    "default": {
      "name": "dsde-perf-broad",
      "zone": "dsde-perf.broadinstitute.org"
    }
  },
  "terraform_state": {
    "default": {
      "state_path_prefix":"workbench",
      "state_bucket": "broad-dsp-terraform-state"
    }
  },
  "clusters": {
    "default": {
      "name": "firecloud-k8s"
    }
  }
}
