{
  "name": "terra",
  "owner": "gmalkov",
  "terraform_image": "us.gcr.io/broad-dsp-gcr-public/config-render:rl_1",
  "intent": "terra infrastructure",
  "production": false,
  "stability": "volatile",
  "global_vars": {
    "env": {
      "VAULT_PATH_PREFIX": "secret/dsde/firecloud/ephemeral/broad-wb-malkov"
    },
    "vault": {
      "policy": "dsde-write"
    }
  },
  "profile_vars": {
    "thurloe-configs": {
      "env": {
        "APP_NAME": "thurloe",
        "FIRECLOUD_DEVELOP_GIT_BRANCH": "dev",
        "SERVICE_GIT_SHA_12_CHAR": "4c11a7177674"
      }
    },
    "opendj-configs": {
      "env": {
        "APP_NAME": "opendj",
        "FIRECLOUD_DEVELOP_GIT_BRANCH": "gm-perf-139-opendj-config"
      }
    }
  },
  "environments": {
    "default": {
      "name": "broad-wb-malkov"
    }
  },
  "projects": {
    "default": {
      "name": "broad-wb-malkov",
      "provider_sa_path": "secret/dsde/terra/envs/common/terra-env-owner-service-account.json"
    },
    "state_bucket": {
      "name": "broad-dsp-techops",
      "provider_sa_path": "secret/dsp/service-accts/dsp-techops/terraform-state-service-account.json"
    }
  },
  "regions": {
    "default": {
      "name": "us-central1",
      "zone_name": "us-central1-a"
    }
  },
  "dns": {
    "default": {
      "name": "dsde-perf-broad",
      "zone": "dsde-perf.broadinstitute.org"
    }
  },
  "terraform_state": {
    "default": {
      "state_path_prefix":"workbench",
      "state_bucket": "broad-dsp-terraform-state"
    }
  },
  "clusters": {
    "default": {
      "name": "firecloud-k8s"
    }
  }
}
