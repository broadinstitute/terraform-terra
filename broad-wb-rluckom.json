{
  "name": "terra",
  "owner": "broad-wb-rluckom",
  "intent": "terra infrastructure",
  "production": false,
  "stability": "volatile",
  "environment": "broad-wb-rluckom",
  "project": "broad-wb-rluckom",
  "region": "us-central1",
  "resource_zone_name": "us-central1-a",
  "dns_zone_name": "dsde-perf-broad",
  "dns_zone": "dsde-perf.broadinstitute.org",
  "global_vars": {
    "env": {
      "VAULT_PATH_PREFIX": "secret/dsde/firecloud/ephemeral/broad-wb-rluckom"
    },
    "vault": {
      "policy": "dsde-write"
    },
    "consul_template": {
      "log_level": "debug"
    },
    "terraform": {
      "docker_image": "us.gcr.io/broad-dsp-gcr-public/config-render:f78ebc56f1b344",
      "providers": {
        "google": {
          "default": {
            "project": "broad-wb-rluckom",
            "vault_sa_path": "secret/dsde/terra/envs/common/terra-env-owner-service-account.json",
            "region": "us-central1"
          }
        }
      },
      "backend": {
        "vault_sa_path": "secret/dsp/service-accts/dsp-techops/terraform-state-service-account.json",
        "state_path_prefix":"workbench",
        "state_bucket": "broad-dsp-terraform-state"
      }
    }
  },
  "profile_vars": {
    "populate-vault": {
      "terraform": {
        "copy_secrets": {
          "jupyter-cert": {
            "from_path": "secret/dsde/firecloud/perf/leonardo/jupyter",
            "to_path": "leonardo/jupyter"
          },
          "whitelist": {
            "from_path": "secret/dsde/firecloud/perf/leonardo/whitelist.json",
            "to_path": "leonardo/whitelist.json"
          },
          "leonardo-secrets": {
            "from_path": "secret/dsde/firecloud/perf/leonardo/secrets",
            "to_path": "leonardo/secrets"
          },
          "leonardo-oauth": {
            "from_path": "secret/dsde/firecloud/perf/leonardo/leonardo-oauth-credential.json",
            "to_path": "leonardo/leonardo-oauth-credential.json"
          }
        }
      }
    },
    "gsuite": {
      "terraform": {
        "providers": {
          "google": {
            "intermediate-domain-delegator": {
              "project": "broad-dsp-techops-dev",
              "vault_sa_path": "secret/devops/terraform/dev/dsp-techops/credentials",
              "region": "us-central1"
            }
          }
        }
      }
    },
    "thurloe-configs": {
      "env": {
        "APP_NAME": "thurloe",
        "FIRECLOUD_DEVELOP_GIT_BRANCH": "dev",
        "SERVICE_GIT_SHA_12_CHAR": "4c11a7177674"
      }
    },
    "leonardo-configs": {
      "env": {
        "APP_NAME": "leonardo",
        "FIRECLOUD_DEVELOP_GIT_BRANCH": "rl-leonardo-configs",
        "SERVICE_GIT_SHA_12_CHAR": "3fb674f22f47"
      }
    },
    "calhoun": {
      "env": {
        "CALHOUN_GIT_COMMIT": "04d2e745c7e4a"
      },
      "terraform": {
        "providers": {
          "google": {
            "app-engine": {
              "project": "broad-dsde-perf",
              "vault_sa_path": "secret/devops/terraform/perf/firecloud/credentials",
              "region": "us-central1"
            }
          }
        }
      }
    },
    "opendj": {
      "terraform": {
        "providers": {
          "google": {
            "dns": {
              "project": "broad-dsde-perf",
              "vault_sa_path": "secret/devops/terraform/perf/firecloud/credentials",
              "region": "us-central1"
            }
          }
        }
      }
    },
    "opendj-configs": {
      "env": {
        "APP_NAME": "opendj",
        "FIRECLOUD_DEVELOP_GIT_BRANCH": "perf-146-sam-opendj"
      }
    },
    "tos": {
      "env": {
        "FUNCTION_ENTRYPOINT": "tos",
        "FUNCTION_GIT_REFSPEC": "rl-configurable-datastore-project",
        "DATASTORE_PROJECT": "broad-dsde-perf"
      },
      "terraform": {"log_level": "TRACE"}
    },
    "sam":{
      "env": {
        "SAM_INSTANCE_NUM_HOSTS": "1"
      },
      "terraform": {
        "providers": {
          "google": {
            "default": {
              "use_local_adc": true
            }
          }
        }
      }
    },
    "sam-configs": {
      "env": {
        "APP_NAME": "sam",
        "FIRECLOUD_DEVELOP_GIT_BRANCH": "perf-146-sam-opendj",
        "SERVICE_GIT_SHA_12_CHAR": "cc43b109de12"
      }
    }
  },
  "clusters": {
    "default": {
      "name": "firecloud-k8s"
    }
  }
}
