{
  "name": "singlecellportal",
  "owner": "broad-singlecellportal-eweitz",
  "intent": "Developer MongoDB VM for Single Cell Portal",
  "production": false,
  "stability": "volatile",
  "environment": "alpha",
  "project": "broad-singlecellportal-eweitz",
  "region": "us-central1",
  "resource_zone_name": "us-central1-a",
  "dns_zone_name": "none",
  "dns_zone": "none",
  "global_vars": {
    "vault": {
      "policy": "dsde-write"
    },
    "terraform": {
      "docker_image": "us.gcr.io/broad-dsp-gcr-public/config-render:7c6c57f6",
      "providers": {
        "google": {
          "default": {
            "project": "broad-singlecellportal-eweitz",
            "vault_sa_path": "secret/dsde/arrow/envs/alpha/terraform-service-account.json",
            "region": "us-central1"
          }
        }
      },
      "backend": {
        "vault_sa_path": "secret/dsp/service-accts/dsp-techops/terraform-state-service-account.json",
        "state_path_prefix":"terra",
        "state_bucket": "broad-dsp-terraform-state"
      }
    }
  },
  "profile_vars": {
    "singlecell-mongo": {
      "env": {
        "SINGLECELL_PROJECT_ID": "broad-singlecellportal-eweitz",
        "SINGLECELL_PROJECT_EDITORS": "group:scp-dev@broadinstitute.org,user:rmeffan@broadinstitute.org,user:rluckom@broadinstitute.org",
        "SINGLECELL_PROJECT_OWNERS": "user:eweitz@broadinstitute.org"
      },
      "terraform": {
        "providers": {
          "google": {
            "default": {
              "vault_sa_path": "secret/kdux/scp/development/eweitz/scp_config.json",
              "use_local_adc": true
            }
          }
        }
      }
    },
    "arrow-firewalls": {
      "env": {
        "ALLOWED_SOURCE_IPS": "0.0.0.0/0"
      }
    }
  }
}
